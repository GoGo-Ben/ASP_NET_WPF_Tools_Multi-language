<Window x:Class="ExcelToXML.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid Margin="0,0,0,20">
        <Label Content="Please Choose file:" HorizontalAlignment="Left" Margin="25,46,0,0" VerticalAlignment="Top" Width="115"/>
        <TextBox Name="txtFIle" HorizontalAlignment="Left" Height="23" Margin="145,46,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="266"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="416,46,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <Button Content="Process&#xD;&#xA;" HorizontalAlignment="Left" Margin="373,102,0,0" VerticalAlignment="Top" Width="102" Height="25" Click="Button_Click_2" RenderTransformOrigin="1.559,0.12"/>
        <Label Content="Sheet:" HorizontalAlignment="Left" Margin="47,102,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.075,-0.077"/>
        <ComboBox x:Name="cmbSheet" HorizontalAlignment="Left" Margin="97,102,0,0" VerticalAlignment="Top" Width="120"/>

        <Label Content="Choose Xaml file:" HorizontalAlignment="Left" Margin="25,158,0,0" VerticalAlignment="Top" Width="115"/>
        <TextBox Name="txtXamlFIlePath" HorizontalAlignment="Left" Margin="145,158,0,116" TextWrapping="Wrap" Height="23" Width="266"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="416,158,0,0" VerticalAlignment="Top" Width="75" Click="btnXamlFile"/>
        <Label Content="Xamls" HorizontalAlignment="Left" Margin="30,210,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.075,-0.077"/>
        <ComboBox x:Name="cmbSheet2" HorizontalAlignment="Left" Margin="97,210,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Content="Process&#xD;&#xA;" HorizontalAlignment="Left" Margin="373,210,0,0" VerticalAlignment="Top" Width="102" Height="25" Click="btnXamlToExcel" RenderTransformOrigin="1.559,0.12"/>

    </Grid>
</Window>
